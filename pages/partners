<script>
    clearActiveMenu();
    $('#partners-nav').addClass('nav-partners-active');
    $('.content-top').attr("id", "partners");
</script>

<div id="partners-content" class="mCustomScrollbar _mCS_1">
    <div class="mCustomScrollBox mCS-light-thick" id="mCSB_1">
        <div class="mCSB_container" >
            <h2>Partners</h2>
            <div id="home_banner">
                <script id="banner_template" type="x-tmpl-mustache">
             
                      <img src="{{photo_url_abs}}" />
        
                </script>
            </div>
        </div>
    </div>
</div>

<script>
    
      $(document).ready(function() {
        
            function renderPageData(){
                var repoList = getRepoList();
                renderBanner('#banner_template','#home_banner',repoList);
            }
            loadMallDataCached(renderPageData);
        });
        
        
        
    function renderBanner(repo_template, repo_txt, repo){
        var item_list = [];
        console.log(repo);
        var repo_template_html = $(repo_template).html();
        Mustache.parse(repo_template_html);   // optional, speeds up future uses
        $.each( repo , function( key, val ) {
            if( val.name == "partners"){
                $.each( val.images , function( key, val ) {
                    var repo_rendered = Mustache.render(repo_template_html,val);
                    console.log(val.images);
                    item_list.push(repo_rendered);
                });
     
            }
            
        });

        $(repo_txt).show();
        $(repo_txt).html(item_list.join(''));

    }

</script>